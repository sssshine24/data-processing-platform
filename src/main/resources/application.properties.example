# Server Port for our business application
server.port=8081

# DataSource for our business data
spring.datasource.url=jdbc:mysql://127.0.0.1:3306/data_platform_db?useUnicode=true&characterEncoding=UTF-8&autoReconnect=true&serverTimezone=Asia/Shanghai
spring.datasource.username=root
spring.datasource.password=YOUR_DATABASE_PASSWORD

# Logging Level
logging.level.com.xxl.job=INFO
logging.level.com.example.dataplatform=DEBUG

# XXL-JOB Executor Configuration
xxl.job.admin.addresses=http://127.0.0.1:8080/xxl-job-admin
xxl.job.accessToken=YOUR_ACCESS_TOKEN
xxl.job.executor.appname=data-platform-executor
xxl.job.executor.address=
xxl.job.executor.ip=
xxl.job.executor.port=9999
xxl.job.executor.logpath=logs/xxl-job/jobhandler
xxl.job.executor.logretentiondays=30

# Redis Configuration
spring.redis.host=127.0.0.1
spring.redis.port=6379
spring.redis.database=0
# spring.redis.password=